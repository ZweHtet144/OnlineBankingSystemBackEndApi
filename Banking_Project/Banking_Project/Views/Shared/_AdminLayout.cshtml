<script>
    function SuccessMessageBox(text) {
        swal({
            title: 'Success!', //Title
            text: text, //Description
            type: 'success',
            allowOutsideClick: false,
            allowEscapeKey: false
        });
    }

    function ErrorMessageBox(text) {
        swal({
            title: 'Error!',
            text: text,
            type: 'error',
            allowOutsideClick: false,
            allowEscapeKey: false
        });
    }

    function WarningMessageBox(text) {
        swal({
            title: 'Warning!',
            text: text,
            type: 'warning',
            allowOutsideClick: false,
            allowEscapeKey: false
        });
    }

    function InfoMessageBox(text) {
        swal({
            title: 'Information!',
            text: text,
            type: 'info',
            allowOutsideClick: false,
            allowEscapeKey: false
        });
    }

    function SuccessMessageBoxWithLink(text, link) {
        swal({
            title: 'Success!',
            text: text,
            type: 'success',
            allowOutsideClick: false,
            confirmButtonColor: '#3085d6',
            confirmButtonText: 'Ok',
            allowEscapeKey: false
        }).then(function () {
            window.location = link;
        });
    }

    function ConfirmMessageBox(title, message, callback) {
        swal({
            title: title,
            text: message,
            type: 'question',
            showCancelButton: true,
            confirmButtonText: 'Yes',
            cancelButtonText: 'No',
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            //confirmButtonClass: 'btn btn-success',
            //cancelButtonClass: 'btn btn-danger',
        }).then((confirmed) => {
            callback(confirmed && confirmed.value == true);
        });
    }
</script>
